  <fsa:DisclosureOfTaxExpenseOnOrdinaryActivities contextRef="ctx1" xml:lang="da">&lt;table xmlns="http://www.w3.org/1999/xhtml" style="border-right: 0px; border-top: 0px; border-left: 0px; width: 650px; border-bottom: 0px; border-collapse: collapse"  width="100%"&gt;&lt;tr&gt;&lt;td width="100%" colspan="4" align="left" valign="middle" &gt;&lt;b&gt;Skat af årets resultat&lt;/b&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td colspan="4"&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td width="100%" colspan="4" align="left" valign="middle" &gt;&lt;b&gt;&lt;/b&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td width="67%" colspan="2" align="left" valign="middle" &gt;Ændring i udskudt skat&lt;/td&gt;&lt;td width="16%" align="right" valign="middle" &gt;-2.132&lt;/td&gt;&lt;td width="17%" align="right" valign="middle" &gt;-3&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td width="59%" &gt;&lt;/td&gt;&lt;td width="8%" &gt;&lt;/td&gt;&lt;td width="16%" align="right" valign="middle" &gt;&lt;b&gt;-2.132&lt;/b&gt;&lt;/td&gt;&lt;td width="17%" align="right" valign="middle" &gt;&lt;b&gt;-3&lt;/b&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td colspan="4"&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;</fsa:DisclosureOfTaxExpenseOnOrdinaryActivities>
